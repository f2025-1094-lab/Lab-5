#include <iostream>
using namespace std;
int main() {
    cout << "Palindrome numbers between 1 and 100 are:";
    for (int num = 1; num <= 100; num++) {
        int temp = num;
        int rev = 0;
        for (int n = temp; n > 0; n = n / 10) {
            int rem = n % 10;
            rev = rev * 10 + rem;
        }
        if (num == rev) {
            cout << num << " ";
        }
    }

    return 0;
}
